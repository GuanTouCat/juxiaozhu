/* 主页 */
.home {
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: white;
  z-index: 1;
  transition: All 0.4s ease;
  -webkit-transition: All 0.4s ease;
}

/* 遮罩层 */
.bg {
  display: none;
  position: fixed;
  top: 0%;
  left: 80%;
  width: 100%;
  height: 100%;
  background-color: black;
  z-index: 1001;
  -moz-opacity: 0.7;
  opacity: 0.70;
  transition:width 2s;
  filter: alpha(opacity=70);
}
